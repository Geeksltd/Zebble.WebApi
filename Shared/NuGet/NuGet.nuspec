<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.WebApi</id>
    <version>4.5.279</version>
    <title>WebApi client for Zebble (Xamarin)</title>
    <description>An easy to use API for consuming Web Api services in Zebble apps.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.WebApi/master/Shared/Icon.png</iconUrl>
    <projectUrl>https://github.com/Geeksltd/Zebble.WebApi</projectUrl>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <copyright>Copyright © 2018 Geeks Ltd - All rights reserved.</copyright>
    <dependencies>
      <group targetFramework="MonoAndroid">
        <dependency id="Newtonsoft.Json" version="9.0.1"></dependency>
        <dependency id="System.Net.Http" version="4.3.2"></dependency>
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Newtonsoft.Json" version="9.0.1"></dependency>
        <dependency id="System.Net.Http" version="4.3.2"></dependency>
      </group>
      <group targetFramework="UAP10">
        <dependency id="Newtonsoft.Json" version="9.0.1"></dependency>
        <dependency id="System.Net.Http" version="4.3.2"></dependency>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\lib\Zebble.WebApi.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.WebApi.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.WebApi.UWP.*" target="lib\UAP10\" />
  </files>
</package>